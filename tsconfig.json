{
  "compilerOptions": {
    "target": "es2021",
    "lib": [
      "es2021",
      "DOM"
    ],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "@cloudflare/workers-types"
    ]
  },
  "include": [
    "worker/**/*",
    "js/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}